{
  "name": "@shm/ui",
  "version": "0.1.0",
  "sideEffects": [
    "*.css"
  ],
  "private": true,
  "types": "./types/index",
  "main": "src/index.tsx",
  "module:jsx": "src",
  "files": [
    "types",
    "dist"
  ],
  "scripts": {
    "build": "tamagui-build --skip-types",
    "watch": "tamagui-build --skip-types --watch",
    "format": "prettier \"**/*.{ts,tsx,md,mdx,json,js}\" --check --ignore-path ../../../.prettierignore",
    "clean": "tamagui-build clean",
    "clean:build": "tamagui-build clean:build",
    "test": "echo NO TESTS",
    "typecheck": "tsc --noEmit",
    "generate": "tamagui generate-themes ./src/themes/theme.ts ./src/themes-generated.ts"
  },
  "dependencies": {
    "@hookform/resolvers": "3.3.1",
    "@shm/shared": "*",
    "@tamagui/animations-moti": "latest",
    "@tamagui/animations-react-native": "latest",
    "@tamagui/checkbox": "latest",
    "@tamagui/cli": "latest",
    "@tamagui/config": "latest",
    "@tamagui/create-theme": "latest",
    "@tamagui/font-inter": "latest",
    "@tamagui/helpers-icon": "latest",
    "@tamagui/lucide-icons": "latest",
    "@tamagui/react-native-media-driver": "latest",
    "@tamagui/shorthands": "latest",
    "@tamagui/stacks": "latest",
    "@tamagui/text": "latest",
    "@tamagui/theme-builder": "latest",
    "@tamagui/themes": "latest",
    "@tamagui/web": "latest",
    "nostr-tools": "1.16.0",
    "react": "18.2.0",
    "react-icons": "4.9.0",
    "react-native-reanimated": "3.7.0",
    "react-virtuoso": "4.6.2",
    "tamagui": "latest"
  },
  "devDependencies": {
    "@shm/prettier": "*",
    "@tamagui/build": "latest",
    "@tamagui/use-did-finish-ssr": "latest"
  }
}
